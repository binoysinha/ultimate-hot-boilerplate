{
  "name": "react-hot-loader-express-minimal-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/zhenyulin/react-hot-loader-express-minimal-boilerplate.git",
  "author": "Zhenyu Lin <zhenyu.lyn@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "nodemon server/server.js --config config/nodemon.json",
    "lint": "eslint . --fix",
    "build:server": "babel server -d dist/server --copy-files --ignore server/**/*.spec.js",
    "build:client": "webpack -p --config config/webpack.prod.js --progress --bail",
    "build": "npm run build:server && npm run build:client",
    "serve": "NODE_PATH=./dist NODE_ENV=production node dist/server/server.js",
    "serve:fresh": "npm run build && npm run serve"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-html-import-to-string": "^0.0.1",
    "babel-preset-env": "^1.5.2",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-import-resolver-node": "^0.3.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jest": "^20.0.3",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "eslint-plugin-react": "^7.0.1",
    "eslint-watch": "^3.1.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.28.0",
    "nodemon": "^1.11.0",
    "react-hot-loader": "3.0.0-beta.6",
    "webpack": "^2.6.1",
    "webpack-dev-middleware-webpack-2": "^1.5.1",
    "webpack-hot-middleware": "^2.18.0"
  },
  "dependencies": {
    "express": "^4.15.3",
    "immutable": "^3.8.1",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-router-redux": "^5.0.0-alpha.6",
    "redux": "^3.6.0",
    "styled-components": "^2.1.0"
  }
}
