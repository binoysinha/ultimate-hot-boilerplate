{
  "extends": [
    "tslint:latest",
    "tslint-react",
    "tslint-eslint-rules",
    "tslint-config-prettier"
  ],
  "rules": {
    "indent": [
      true,
      "spaces"
    ],
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "member-ordering": [
      false
    ],
    "object-literal-sort-keys": false,
    "no-shadowed-variable": false,
    "no-console": [
      false
    ]
  }
}
